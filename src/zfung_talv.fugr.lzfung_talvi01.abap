MODULE user_command_9200 INPUT.

  CASE ok_code.
    WHEN '&SAVE'.
      editor->get_textstream( IMPORTING text = <text> ).
      MESSAGE TEXT-s01 TYPE 'S'.
  ENDCASE.

ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  9200_EXIT  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE 9200_exit INPUT.

  editor->free( ).
  editor_container->free( ).

  FREE editor.
  FREE editor_container.

  LEAVE TO SCREEN 0.

ENDMODULE.
